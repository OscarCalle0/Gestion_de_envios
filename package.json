{
    "name": "cm-dineros-plantilla-nodejs-gke-azure",
    "version": "1.0.0",
    "main": "dist/infrastructure/api/Server.js",
    "description": "Repositorio de la plantilla NodeJS con Fastify para crear microservicios en Google Kubernetes Engine con pipelines en Azure",
    "license": "UNLICENSED",
    "private": true,
    "author": {
        "email": "it@coordinadora.com",
        "name": "Coordinadora Mercantil S.A"
    },
    "scripts": {
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "infra-as-code": "echo Infrastructure as code",
        "lint": "eslint . --fix",
        "format": "prettier --write \"./src/**/*.ts\"",
        "format-check": "prettier --list-different \"./src/**/*.ts\"",
        "dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/infrastructure/api/Server.js\"",
        "start": "node dist/infrastructure/api/Server.js",
        "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/infrastructure/api/Server.js\"",
        "test": "jest --color --verbose --no-cache",
        "coverage": "jest --coverage",
        "release": "standard-version",
        "husky:install": "husky install",
        "prepare": "husky"
    },
    "dependencies": {
        "@fastify/cors": "11.0.0",
        "@fastify/formbody": "8.0.2",
        "@fastify/helmet": "13.0.1",
        "@fastify/static": "8.1.1",
        "@fastify/swagger": "9.4.2",
        "@fastify/swagger-ui": "5.2.2",
        "@google-cloud/firestore": "7.11.0",
        "dotenv": "16.4.7",
        "fastify": "5.2.1",
        "inversify": "7.1.0",
        "ioredis": "^5.9.2",
        "joi": "17.13.3",
        "module-alias": "2.2.3",
        "pg-promise": "11.11.0",
        "reflect-metadata": "0.2.2"
    },
    "devDependencies": {
        "@commitlint/cli": "19.8.0",
        "@commitlint/config-conventional": "19.8.0",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "29.5.14",
        "@types/node": "22.13.10",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "8.26.1",
        "@typescript-eslint/parser": "8.26.1",
        "eslint": "9.22.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-prettier": "5.2.3",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "mock-cloud-firestore": "0.12.0",
        "prettier": "3.5.3",
        "rimraf": "6.0.1",
        "standard-version": "9.5.0",
        "supertest": "^7.2.2",
        "ts-jest": "29.2.6",
        "ts-node": "10.9.2",
        "tsc-watch": "6.2.1",
        "typescript": "5.8.2"
    },
    "_moduleAliases": {
        "@application": "dist/application",
        "@domain": "dist/domain",
        "@infrastructure": "dist/infrastructure",
        "@configuration": "dist/configuration",
        "@util": "dist/util"
    }
}
